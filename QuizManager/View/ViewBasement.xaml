<Page
    x:Class="QuizManager.View.ViewBasement"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Core="using:Microsoft.Xaml.Interactions.Core"
    xmlns:Interactivity="using:Microsoft.Xaml.Interactivity"
    xmlns:ViewModel="using:QuizManager.ViewModel"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:QuizManager.View"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
    mc:Ignorable="d">
    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>


    <Page.DataContext>
        <ViewModel:ViewModelBasement />
    </Page.DataContext>


    <Grid Background="#354357">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.5*" />
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />


        </Grid.ColumnDefinitions>

        <StackPanel Grid.RowSpan="2" Padding="5">
            <StackPanel
                BorderBrush="#282b30"
                BorderThickness="0,0,0,1"
                Orientation="Horizontal">
                <Button
                    Background="{x:Null}"
                    Content="&#xE716;;"
                    FontFamily="Segoe MDL2 Assets"
                    FontSize="29"
                    Foreground="White"
                    Style="{StaticResource StandardMenuButton}">
                    <Button.Flyout>
                        <Flyout FlyoutPresenterStyle="{StaticResource FlyoutPresenterStyle}" Placement="Right">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />

                                    <RowDefinition />
                                </Grid.RowDefinitions>

                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    FontSize="20"
                                    Foreground="White"
                                    Text="Dato" />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    FontSize="20"
                                    Foreground="White"
                                    Text="Event navn" />
                                <Button
                                    x:Name="Tilføj_event_knap"
                                    Grid.Row="2"
                                    HorizontalAlignment="Center"
                                    Content="Tilføj Event"
                                    Foreground="White" />

                                <CalendarDatePicker
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    PlaceholderText="Event dato:" />




                                <TextBox
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    PlaceholderText="Event Navn:" />







                            </Grid>
                        </Flyout>
                    </Button.Flyout>
                </Button>
                <TextBlock
                    FontSize="20"
                    Foreground="White"
                    Text="Tilføj Event" />
            </StackPanel>
            <ListView ItemsSource="{Binding Dates}" SelectedItem="{Binding SelectedDate, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <StackPanel
                            Padding="0,5,0,10"
                            BorderBrush="Black"
                            BorderThickness="0,0,0,1">
                            <TextBlock
                                FontSize="20"
                                Foreground="White"
                                Text="{Binding Date}" />
                            <TextBlock
                                FontSize="20"
                                Foreground="White"
                                Text="{Binding EventType}" />
                        </StackPanel>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackPanel>



        <!--  GridView Groups  Laura  -->



        <StackPanel
            x:Name="Hold_Grid"
            Grid.Row="0"
            Grid.Column="1"
            Padding="5">
            <StackPanel
                BorderBrush="#282b30"
                BorderThickness="0,0,0,1"
                Orientation="Horizontal">
                <Button
                    x:Name="SearchButton"
                    Background="{x:Null}"
                    Content="&#xE7EE;"
                    FontFamily="Segoe MDL2 Assets"
                    FontSize="29"
                    FontWeight="Bold"
                    Foreground="White"
                    RelativePanel.AlignRightWithPanel="True"
                    Style="{StaticResource StandardMenuButton}">

                    <Button.Flyout>
                        <Flyout FlyoutPresenterStyle="{StaticResource FlyoutPresenterStyle}" Placement="Right">
                            <StackPanel
                                x:Name="Tilføj_gruppe"
                                Padding="10"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top">
                                <StackPanel
                                    x:Name="Tilføj_gruppe_info"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal">
                                    <StackPanel Margin="0,0,5,0">
                                        <TextBlock Foreground="White" Text="Holdnavn:" />
                                        <TextBlock Foreground="White" Text="Tlf nummer:" />
                                        <TextBlock Foreground="White" Text="Antal deltagere:" />
                                        <TextBlock Foreground="White" Text="Bord Nr: " />
                                    </StackPanel>
                                    <StackPanel>
                                        <TextBox Text="{Binding TeamName, Mode=TwoWay}" />
                                        <TextBox Text="{Binding PhoneNumber, Mode=TwoWay}" />
                                        <TextBox Text="{Binding Participants, Mode=TwoWay}" />
                                        <TextBox Text="{Binding TableNr, Mode=TwoWay}" />
                                    </StackPanel>
                                </StackPanel>
                                <Button
                                    x:Name="Tilføj_gruppe_knap"
                                    HorizontalAlignment="Center"
                                    Content="Tilføj Gruppe"
                                    Foreground="White">
                                    <Interactivity:Interaction.Behaviors>
                                        <Core:EventTriggerBehavior EventName="Click">
                                            <Core:CallMethodAction MethodName="AddGroup" TargetObject="{Binding Mode=OneWay}" />
                                        </Core:EventTriggerBehavior>
                                    </Interactivity:Interaction.Behaviors>
                                </Button>
                            </StackPanel>

                        </Flyout>
                    </Button.Flyout>
                </Button>
                <TextBlock
                    FontSize="20"
                    Foreground="White"
                    Text="Tilføj en gruppe" />
            </StackPanel>
            <Grid MaxHeight="300">
                <GridView
                    x:Name="Holdnavn_Grid"
                    ItemsSource="{Binding SelectedDate.Groups, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                    ScrollViewer.VerticalScrollBarVisibility="Visible"
                    ScrollViewer.VerticalScrollMode="Enabled"
                    SelectedItem="{Binding SelectedGroup, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">

                    <GridView.ItemTemplate>
                        <DataTemplate>
                            <Border
                                Width="auto"
                                Margin="1"
                                BorderBrush="#FF060606"
                                BorderThickness="1">
                                <StackPanel x:Name="Gridview_datatemplate" Margin="5">
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Margin="0,0,0,0" Text="{Binding TeamName}" />
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="Bord nummer #" />
                                        <TextBlock Margin="0,0,0,0" Text="{Binding TableNr}" />
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="Deltagere:" />
                                        <TextBlock Margin="5,0,0,0" Text="{Binding Participants, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                    </StackPanel>
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock Text="{Binding AllPaidMessage, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                        <TextBlock Text="{Binding AllAttendingMessage, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                    </StackPanel>
                                </StackPanel>
                            </Border>
                        </DataTemplate>
                    </GridView.ItemTemplate>
                </GridView>
            </Grid>
        </StackPanel>
        <!--  Tilføjelse af nye hold  -->
        <!--  Slut tilføjelse af hold  -->


        <!--  End of gridview  -->



                <!--  Edit groups - Laura  -->
                <Border  BorderBrush="Black" BorderThickness="1" Grid.Column="1" >
                    <StackPanel
            x:Name="Hold_Info"
            Grid.Row="0"

            Grid.Column="2"
            Padding="5">
            <StackPanel

                x:Name="Holdnavn"
                Margin="0,0,0,20"
                Orientation="Horizontal">
                            <TextBlock
                    VerticalAlignment="Center"
                    FontSize="14"
                    Text="Team" />
                            <TextBlock
                    Margin="5,0,0,0"
                    VerticalAlignment="Center"
                    FontSize="20"
                    Text="{Binding SelectedGroup.TeamName}" />
                        </StackPanel>
                        <StackPanel x:Name="Telefon_Nummer" Orientation="Horizontal">
                            <TextBlock VerticalAlignment="Center" Text="Telefon nummer:" />
                            <TextBlock Margin="5,0,0,0" Text="{Binding SelectedGroup.PhoneNumber}" />
                        </StackPanel>
                        <StackPanel x:Name="Antal_Deltager" Orientation="Horizontal">
                            <TextBlock VerticalAlignment="Center" Text="Antal deltagere: " />
                            <Button Margin="10,0,0,0" Content="-">
                                <Interactivity:Interaction.Behaviors>
                                    <Core:EventTriggerBehavior EventName="Click">
                                        <Core:CallMethodAction MethodName="RemoveOneParticipant" TargetObject="{Binding SelectedGroup, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                    </Core:EventTriggerBehavior>
                                </Interactivity:Interaction.Behaviors>
                            </Button>
                            <TextBox
                    Margin="5,0,0,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Text="{Binding SelectedGroup.Participants, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                            <Button Margin="10,0,0,0" Content="+">
                                <Interactivity:Interaction.Behaviors>
                                    <Core:EventTriggerBehavior EventName="Click">
                                        <Core:CallMethodAction MethodName="AddOneParticipant" TargetObject="{Binding SelectedGroup}" />
                                    </Core:EventTriggerBehavior>
                                </Interactivity:Interaction.Behaviors>
                            </Button>
                        </StackPanel>
                        <StackPanel x:Name="Fremmødte" Orientation="Horizontal">
                            <TextBlock VerticalAlignment="Center" Text="Fremmødte: " />
                            <Button Margin="10,0,0,0" Content="-">
                                <Interactivity:Interaction.Behaviors>
                                    <Core:EventTriggerBehavior EventName="Click">
                                        <Core:CallMethodAction MethodName="RemoveOneAttending" TargetObject="{Binding SelectedGroup}" />
                                    </Core:EventTriggerBehavior>
                                </Interactivity:Interaction.Behaviors>
                            </Button>
                            <TextBox
                    Margin="5,0,0,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Text="{Binding SelectedGroup.NumberOfAttendingParticipants, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                            <Button Margin="10,0,0,0" Content="+">
                                <Interactivity:Interaction.Behaviors>
                                    <Core:EventTriggerBehavior EventName="Click">
                                        <Core:CallMethodAction MethodName="AddOneAttending" TargetObject="{Binding SelectedGroup}" />
                                    </Core:EventTriggerBehavior>
                                </Interactivity:Interaction.Behaviors>
                            </Button>
                        </StackPanel>
                        <StackPanel x:Name="Bord_Nummer" Orientation="Horizontal">
                            <TextBlock Text="Bord nr # " />
                            <TextBlock Text="{Binding SelectedGroup.TableNr}" />
                        </StackPanel>
                        <StackPanel x:Name="Betalt" Orientation="Horizontal">
                            <TextBlock VerticalAlignment="Center" Text="Betalt: " />
                            <Button Margin="10,0,0,0" Content="-">
                                <Interactivity:Interaction.Behaviors>
                                    <Core:EventTriggerBehavior EventName="Click">
                                        <Core:CallMethodAction MethodName="RemoveOnePayment" TargetObject="{Binding SelectedGroup}" />
                                    </Core:EventTriggerBehavior>
                                </Interactivity:Interaction.Behaviors>
                            </Button>
                            <TextBox
                    Margin="5,0,0,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Text="{Binding SelectedGroup.NumberOfPayments, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                <Button Margin="10,0,0,0" Content="+">
                    <Interactivity:Interaction.Behaviors>
                        <Core:EventTriggerBehavior EventName="Click">
                            <Core:CallMethodAction MethodName="AddOnePayment" TargetObject="{Binding SelectedGroup}" />
                        </Core:EventTriggerBehavior>
                    </Interactivity:Interaction.Behaviors>
                </Button>
            </StackPanel>
            <Button
                x:Name="SletGruppe"
                Margin="0,100,0,0"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Content="Slet Gruppe">
                <Interactivity:Interaction.Behaviors>
                    <Core:EventTriggerBehavior EventName="Click">
                        <Core:CallMethodAction MethodName="RemoveGroup" TargetObject="{Binding Mode=OneWay}" />
                    </Core:EventTriggerBehavior>
                </Interactivity:Interaction.Behaviors>
            </Button>

                           
                    </StackPanel>
                </Border>
            </Grid>

        </StackPanel>



        <!--  Rasmus  -->
        <StackPanel
            x:Name="Event_Plads"
            Grid.Row="1"
            Grid.Column="1"
            Grid.ColumnSpan="2">

            <TextBlock
                FontSize="36"
                Foreground="#FF0D0D0D"
                Text="Informationer For Det Valgte Event"
                TextWrapping="Wrap" />
            <StackPanel
                x:Name="Events"
                Margin="0,20,0,0"
                Orientation="Horizontal">

                <StackPanel x:Name="Events1" Orientation="Vertical">
                    <StackPanel
                        x:Name="TotalSeats"
                        Margin="0"
                        Orientation="Horizontal">
                        <TextBlock
                            Width="291"
                            FontSize="22"
                            Text="Antal Stole I Alt:"
                            TextWrapping="Wrap" />
                        <TextBox
                            x:Name="Antal_Pladser"
                            Width="120"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            FocusVisualPrimaryBrush="#FF9B0A0A"
                            FontSize="24"
                            Foreground="#FF050589"
                            Text="{Binding SelectedDate.TotalSeats, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            TextWrapping="Wrap" />
                        <Button
                            x:Name="Add_Seat"
                            Width="30"
                            Height="42"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="+">
                            <Interactivity:Interaction.Behaviors>
                                <Core:EventTriggerBehavior EventName="Click">
                                    <Core:CallMethodAction MethodName="AddSeat" TargetObject="{Binding Mode=OneWay}" />
                                </Core:EventTriggerBehavior>
                            </Interactivity:Interaction.Behaviors>

                        </Button>
                        <Button
                            x:Name="Subtract_Seat"
                            Width="30"
                            Height="42"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="-">
                            <Interactivity:Interaction.Behaviors>
                                <Core:EventTriggerBehavior EventName="Click">
                                    <Core:CallMethodAction MethodName="SubtractSeat" TargetObject="{Binding Mode=OneWay}" />
                                </Core:EventTriggerBehavior>
                            </Interactivity:Interaction.Behaviors>

                        </Button>
                    </StackPanel>
                    <StackPanel
                        x:Name="TotalRevenue"
                        Margin="0,12,0,0"
                        Orientation="Horizontal">
                        <TextBlock
                            Width="291"
                            FontSize="22"
                            Text="Total Omsætning:"
                            TextWrapping="Wrap" />
                        <TextBox
                            x:Name="Antal_Pladser5"
                            Width="120"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            FocusVisualPrimaryBrush="#FF9B0A0A"
                            FontSize="24"
                            Foreground="#FF050589"
                            Text="{Binding SelectedDate.TotalRevenue, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            TextWrapping="Wrap" />
                        <Button
                            x:Name="Subtract_Seat5"
                            Width="60"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="Enter">
                            <Interactivity:Interaction.Behaviors>
                                <Core:EventTriggerBehavior EventName="Click">
                                    <Core:CallMethodAction />
                                </Core:EventTriggerBehavior>
                            </Interactivity:Interaction.Behaviors>

                        </Button>
                    </StackPanel>
                    <StackPanel
                        x:Name="Revenue20To24"
                        Margin="0,12,0,0"
                        Orientation="Horizontal">
                        <TextBlock
                            Width="291"
                            FontSize="22"
                            Text="Omsætning Fra 20 - 24:"
                            TextWrapping="Wrap" />
                        <TextBox
                            x:Name="Antal_Pladser2"
                            Width="120"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            FocusVisualPrimaryBrush="#FF9B0A0A"
                            FontSize="24"
                            Foreground="#FF050589"
                            Text="{Binding SelectedDate.Revenue20To24, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            TextWrapping="Wrap" />
                        <Button
                            x:Name="Add_Seat2"
                            Width="60"
                            Height="43"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="Enter">
                            <Interactivity:Interaction.Behaviors>
                                <Core:EventTriggerBehavior EventName="Click">
                                    <Core:CallMethodAction />
                                </Core:EventTriggerBehavior>
                            </Interactivity:Interaction.Behaviors>

                        </Button>
                    </StackPanel>
                    <StackPanel
                        x:Name="Revenue24ToClose1"
                        Margin="0,12,0,0"
                        Orientation="Horizontal">
                        <TextBlock
                            Width="291"
                            FontSize="22"
                            Text="Omsætning Fra 24 - Lukketid:"
                            TextWrapping="Wrap" />
                        <TextBox
                            x:Name="Antal_Pladser1"
                            Width="120"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            FocusVisualPrimaryBrush="#FF9B0A0A"
                            FontSize="24"
                            Foreground="#FF050589"
                            Text="{Binding SelectedDate.Revenue24ToClose, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            TextWrapping="Wrap" />
                        <Button
                            x:Name="Add_Seat1"
                            Width="60"
                            Height="46"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="Enter">
                            <Interactivity:Interaction.Behaviors>
                                <Core:EventTriggerBehavior EventName="Click">
                                    <Core:CallMethodAction />
                                </Core:EventTriggerBehavior>
                            </Interactivity:Interaction.Behaviors>

                        </Button>
                    </StackPanel>
                </StackPanel>
                <StackPanel
                    x:Name="Events2"
                    Margin="180,0,0,0"
                    Orientation="Vertical">
                    <StackPanel
                        x:Name="TotalParticipants"
                        Margin="0"
                        Orientation="Horizontal">
                        <TextBlock
                            Width="270"
                            FontSize="22"
                            Text="Deltagere I Alt:"
                            TextWrapping="Wrap" />
                        <TextBox
                            x:Name="Antal_Pladser4"
                            Width="120"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            FocusVisualPrimaryBrush="#FF9B0A0A"
                            FontSize="24"
                            Foreground="#FF050589"
                            Text="{Binding SelectedDate.TotalParticipants, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            TextWrapping="Wrap" />
                        <Button
                            x:Name="Add_Seat4"
                            Width="60"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="Check">
                            <Interactivity:Interaction.Behaviors>
                                <Core:EventTriggerBehavior EventName="Click">
                                    <Core:CallMethodAction MethodName="CheckTotalParticipants" TargetObject="{Binding Mode=OneWay}" />
                                </Core:EventTriggerBehavior>
                            </Interactivity:Interaction.Behaviors>

                        </Button>
                    </StackPanel>
                    <StackPanel
                        x:Name="TotalRevenueQuiz"
                        Margin="0,12,0,0"
                        Orientation="Horizontal">
                        <TextBlock
                            Width="270"
                            FontSize="22"
                            Text="Total Quiz Omsætning:"
                            TextWrapping="Wrap" />
                        <TextBox
                            x:Name="Antal_Pladser3"
                            Width="120"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            FocusVisualPrimaryBrush="#FF9B0A0A"
                            FontSize="24"
                            Foreground="#FF050589"
                            Text="{Binding SelectedDate.TotalRevenueQuiz, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            TextWrapping="Wrap" />
                        <Button
                            x:Name="Add_Seat3"
                            Width="60"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="Enter">
                            <Interactivity:Interaction.Behaviors>
                                <Core:EventTriggerBehavior EventName="Click">
                                    <Core:CallMethodAction />
                                </Core:EventTriggerBehavior>
                            </Interactivity:Interaction.Behaviors>

                        </Button>
                    </StackPanel>
                    <StackPanel
                        x:Name="TotalRevenueQuizParticipants"
                        Margin="0,12,0,0"
                        Orientation="Horizontal">
                        <TextBlock
                            Width="270"
                            FontSize="22"
                            Text="Total Omsætning                     For Quiz Deltagere: "
                            TextWrapping="Wrap" />
                        <TextBox
                            x:Name="Antal_Pladser7"
                            Width="120"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            FocusVisualPrimaryBrush="#FF9B0A0A"
                            FontSize="24"
                            Foreground="#FF050589"
                            Text="{Binding SelectedDate.TotalRevenueQuizParticipants, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            TextWrapping="Wrap" />
                        <Button
                            x:Name="Add_Seat7"
                            Width="60"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="Enter">
                            <Interactivity:Interaction.Behaviors>
                                <Core:EventTriggerBehavior EventName="Click">
                                    <Core:CallMethodAction />
                                </Core:EventTriggerBehavior>
                            </Interactivity:Interaction.Behaviors>

                        </Button>
                    </StackPanel>
                    <StackPanel
                        x:Name="RevenueQuizOffers"
                        Margin="0"
                        Orientation="Horizontal">
                        <TextBlock
                            Width="270"
                            FontSize="22"
                            Text="Omsætning Af Quiz Tilbud: "
                            TextWrapping="Wrap" />
                        <TextBox
                            x:Name="Antal_Pladser6"
                            Width="120"
                            Height="45"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            FocusVisualPrimaryBrush="#FF9B0A0A"
                            FontSize="24"
                            Foreground="#FF050589"
                            Text="{Binding SelectedDate.RevenueQuizOffers, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            TextWrapping="Wrap" />
                        <Button
                            x:Name="Add_Seat6"
                            Width="60"
                            Height="44"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Content="Enter">
                            <Interactivity:Interaction.Behaviors>
                                <Core:EventTriggerBehavior EventName="Click">
                                    <Core:CallMethodAction />
                                </Core:EventTriggerBehavior>
                            </Interactivity:Interaction.Behaviors>

                        </Button>
                    </StackPanel>

                </StackPanel>

            </StackPanel>

        </StackPanel>
        <!--    -->






    </Grid>

</Page>
